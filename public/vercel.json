{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/404",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/404",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/404.html"
    }
  ]
}